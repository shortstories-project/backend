{
  "parser": "babel-eslint",
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "node": true,
    "es6": true,
    "mocha": true
  },
  "rules": {
    "no-return-await": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "import/no-mutable-exports": "off",
    "consistent-return": "off",
    "dot-notation":"off",
    "import/no-extraneous-dependencies": "off",
    "import/prefer-default-export": "off"
  }
}
